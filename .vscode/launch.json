{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch flow program",
            "type": "node",
            "request": "launch",
            "sourceMaps": true,
            "preLaunchTask": "flow-src",
            "program": "${file}",
            "outFiles": [
                "${workspaceRoot}/build/src/**/*.js"
            ]
        },
        {
            "name": "Launch flow-test program",
            "type": "node",
            "request": "launch",
            "sourceMaps": true,
            "preLaunchTask": "flow-test",
            "program": "${file}",
            "outFiles": [
                "${workspaceRoot}/build/test/**/*.js"
            ]
        }
    ]
}